		<typeAlias alias="SubUserVo" type="chatMatching.vo.SubUserVo" />
		<typeAlias alias="IntroductionVo" type="myBlogErEe.vo.IntroductionVo" />
		<typeAlias alias="CareerVo" type="myBlogErEe.vo.CareerVo" />
		<typeAlias alias="RankingVo" type="ranking.vo.RankingVo" />
		<typeAlias alias="AdverVo" type="advertisement.vo.AdverVo" />
